{
  "expo": {
    "name": "shoepalace",
    "slug": "shoepalace",
    "scheme": "shoepalace",
    "version": "1.0.0",
    "orientation": "portrait",

    "userInterfaceStyle": "light",

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "packagerOpts": {
      "sourceExts": ["js", "json", "ts", "tsx", "jsx"],
      "config": "metro.config.js"
    }
  }
}
